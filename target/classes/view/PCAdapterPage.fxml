<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane prefHeight="324.0" prefWidth="497.0" stylesheets="@comix.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.PCAdapterController">
   <content>
      <AnchorPane prefHeight="348.0" prefWidth="475.0">
         <children>
            <Button fx:id="csvExportButton" layoutX="15.0" layoutY="14.0" mnemonicParsing="false" onAction="#exportToCSV" prefHeight="26.0" prefWidth="135.0" text="Export to CSV" />
            <Button fx:id="xmlExportButton" layoutX="15.0" layoutY="45.0" mnemonicParsing="false" onAction="#exportToXML" prefHeight="26.0" prefWidth="135.0" text="Export to XML" />
            <Button fx:id="jsonExportButton" layoutX="15.0" layoutY="76.0" mnemonicParsing="false" onAction="#exportToJSON" prefHeight="26.0" prefWidth="135.0" text="Export to JSON" />
         </children>
      </AnchorPane>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="OK" />
   </buttonTypes>
</DialogPane>
